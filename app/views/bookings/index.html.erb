<h1>Bookings#index</h1>
<p>Find me in app/views/bookings/index.html.erb</p>
<%= @bookings_tutor %>
<%= @bookings_student %>
<% @all_bookings.each do |booking| %>
  <p> <%= booking.start %> </p>
  <div class="btn-toolbar">
    <%= link_to('Edit', edit_booking_path(booking), class: 'btn btn-warning me-2')%>
    <%= button_to('Delete', booking, class: 'btn btn-danger', method: :delete, form: { data: { turbo_confirm: "Are you sure you want to delete this booking ?" } })%>
  </div>
<% end %>
